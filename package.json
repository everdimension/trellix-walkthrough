{
  "name": "trellix-walkthrough",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "node --inspect ./node_modules/.bin/react-router dev",
    "dev:noinspect": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "postinstall": "npm run prisma:migrate",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "bcrypt": "^6.0.0",
    "get-error": "^1.0.1",
    "isbot": "^5.1.27",
    "memoize-one": "^6.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "prisma": "^6.9.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
